  <body>
    <div id="map"></div>
    <script type="text/javascript">

var map;
function initMap() {
  map = new google.maps.Map(document.getElementById('map'), {
    center: {lat: -12.397, lng: -76.644},
    zoom: 12
  });
  map.data.loadGeoJson('https://raw.githubusercontent.com/eespinozagis/GIS/master/Zonas_LIMA.geojson');
  
}




    </script>
    <script async defer
      src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAh9hGyDI-iogjIt-Xd51YWLBnG6eZdNCU&callback=initMap">
    </script>
  </body>
</html>
